[{
	"type": "get",
	"pattern": "/site",
	"handler": [
		{
			"controller": "siteController",
			"method": "addView"	
		}
	]
}, {
	"type": "post",
	"pattern": "/site/add",
	"handler": [{
			"controller": "siteController",
			"method": "add"
	}]
}, {
	"type": "all",
	"pattern": "/site/del/:_id",
	"handler": [
		{
			"controller": "siteController",
			"method": "del"		
		}
	]
}]
